# GraphWiz Trader Environment Variables
# Copy this file to .env and update with your settings

# Neo4j
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your_neo4j_password

# Exchange API Keys (Germany-Compliant)

# Kraken (MiCA-licensed August 2025) - PRIMARY EXCHANGE
KRAKEN_API_KEY=your_kraken_api_key_here
KRAKEN_API_SECRET=your_kraken_api_secret_here

# One Trading / Bitpanda Pro (MiCA-licensed January 2025) - SECONDARY EXCHANGE
ONETRADING_API_KEY=your_onetrading_api_key_here
ONETRADING_API_SECRET=your_onetrading_api_secret_here

# Binance (NOT LICENSED in Germany - DO NOT USE)
BINANCE_API_KEY=your_binance_api_key
BINANCE_API_SECRET=your_binance_api_secret

# OpenAI API (for AI agents)
OPENAI_API_KEY=your_openai_api_key

# Anthropic API (alternative AI provider)
ANTHROPIC_API_KEY=your_anthropic_api_key

# Trading Mode
TRADING_MODE=test  # Options: test, live

# Log Level
LOG_LEVEL=INFO
